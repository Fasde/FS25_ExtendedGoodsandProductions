    <parentFile xmlFilename="$data/objects/pallets/flourPallet/flourBoxPallet.xml">
        <attributes>
            <set path="vehicle.storeData.name"        value="$l10n_fillType_peaFlour"/>
            <set path="vehicle.storeData.price"       value="1925"/>
            <set path="vehicle.storeData.image"       value="$data/store/store_empty.png"/>
